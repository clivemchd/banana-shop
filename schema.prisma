datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Image {
	id String @id @default(uuid())
	url String
	description String
	users Users @relation(fields: [userId], references: [id])
	userId String
}

model Users {
	id String @id @default(uuid())
	images Image[]
	isAdmin  Boolean @default(false)
	email String? @unique
	username String? @unique
}