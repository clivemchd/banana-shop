{
  "compilerOptions": {
    "module": "esnext",
    "composite": true,
    "target": "esnext",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "allowJs": true,
    "outDir": ".wasp/out/user"
  },
  "include": ["src"]
}
