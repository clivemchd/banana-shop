# Database URL for PostgreSQL
# You can get this from your database provider (e.g., PostgreSQL, Supabase, Railway, etc.)
DATABASE_URL="postgresql://username:password@localhost:5432/nanostudio"

# Wasp Web Client URL (used for redirect URLs)
WASP_WEB_CLIENT_URL="http://localhost:3000"

# Stripe Configuration (Test Mode)
# Get these from your Stripe dashboard: https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY_TEST="sk_test_..."
STRIPE_PUBLISHABLE_KEY_TEST="pk_test_..."
STRIPE_WEBHOOK_SECRET_TEST="whsec_..."

# Stripe Configuration (Production Mode)
# Only set these when deploying to production
# STRIPE_SECRET_KEY_LIVE="sk_live_..."
# STRIPE_PUBLISHABLE_KEY_LIVE="pk_live_..."
# STRIPE_WEBHOOK_SECRET_LIVE="whsec_..."

# Stripe Price IDs (from your Stripe dashboard)
# Create products and prices in Stripe, then use their IDs here
STRIPE_STARTER_PRICE_ID="price_..."
STRIPE_PRO_PRICE_ID="price_..."
STRIPE_BUSINESS_PRICE_ID="price_..."
STRIPE_CREDITS_PRICE_ID="price_..."

# FAL AI Configuration (if you're using AI features)
FAL_KEY="your_fal_api_key_here"

# Google OAuth Configuration (if using Google login)
GOOGLE_CLIENT_ID="your_google_client_id"
GOOGLE_CLIENT_SECRET="your_google_client_secret"

# Email Configuration (for development, Wasp uses a dummy provider)
# In production, you'd configure SendGrid, Mailgun, or another provider